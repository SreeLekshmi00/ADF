{
	"name": "KeyVaultTryAll",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_keyvaultADLSFiles",
						"type": "DatasetReference"
					},
					"name": "OnPremSQL2ADLSFile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_JoinOutput",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "ds_JoinOutput",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "split1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          column_a as integer,",
				"          trans_id as string,",
				"          account_id as string,",
				"          type as string,",
				"          operation as string,",
				"          amount as float,",
				"          balance as float,",
				"          symbol as string,",
				"          bank as string,",
				"          account as string,",
				"          year as string,",
				"          month as string,",
				"          day as string,",
				"          fulldate as date,",
				"          fulltime as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> OnPremSQL2ADLSFile",
				"OnPremSQL2ADLSFile split(column_a <100,",
				"     disjoint: false) ~> split1@(NewTable1, NewTable2)",
				"split1@NewTable2 aggregate(groupBy(fulldate),",
				"     amount = sum(amount)) ~> aggregate1",
				"split1@NewTable1 filter(amount>5000) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          column_a as string,",
				"          trans_id as string,",
				"          account_id as string,",
				"          type as string,",
				"          operation as string,",
				"          amount as string,",
				"          balance as string,",
				"          symbol as string,",
				"          bank as string,",
				"          account as string,",
				"          year as string,",
				"          month as string,",
				"          day as string,",
				"          fulldate as string,",
				"          fulltime as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          column_a as string,",
				"          trans_id as string,",
				"          account_id as string,",
				"          type as string,",
				"          operation as string,",
				"          amount as string,",
				"          balance as string,",
				"          symbol as string,",
				"          bank as string,",
				"          account as string,",
				"          year as string,",
				"          month as string,",
				"          day as string,",
				"          fulldate as string,",
				"          fulltime as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink2"
			]
		}
	}
}